<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    version="2.5">
 
 	<servlet>
	   <servlet-name>MainMenu</servlet-name>
	   <servlet-class>com.bandwidth.sdk.xml.examples.MainMenuServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
	   <servlet-name>MainMenu</servlet-name>
	   <url-pattern>/mainmenu</url-pattern>
	</servlet-mapping>
	
 	<servlet>
	   <servlet-name>Transfer</servlet-name>
	   <servlet-class>com.bandwidth.sdk.xml.examples.TransferServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
	   <servlet-name>Transfer</servlet-name>
	   <url-pattern>/transfer</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Gather</servlet-name>
		<servlet-class>com.bandwidth.sdk.xml.examples.GatherServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Gather</servlet-name>
		<url-pattern>/gather</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>GatherComplete</servlet-name>
		<servlet-class>com.bandwidth.sdk.xml.examples.GatherCompleteServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>GatherComplete</servlet-name>
		<url-pattern>/gatherComplete</url-pattern>
	</servlet-mapping>
</web-app>
